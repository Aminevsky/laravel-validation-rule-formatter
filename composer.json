{
  "name": "aminevsky/laravel-validation-rule-formatter",
  "description": "A Formatter of Laravel Validation Rule",
  "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aminevsky",
            "email": "amigomr@gmail.com"
        }
    ],
    "require": {
      "php": "^7.3"
    },
    "require-dev": {
      "orchestra/testbench": "^6.0",
      "phpstan/phpstan": "^0.12.43"
    },
    "autoload": {
      "psr-4": {
        "Aminevsky\\ValidationRuleFormatter\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Aminevsky\\ValidationRuleFormatter\\Tests\\": "tests/"
      }
    },
    "extra": {
      "laravel": {
        "providers": [
          "Aminevsky\\ValidationRuleFormatter\\ValidationRuleFormatterServiceProvider"
        ],
        "aliases": {
          "ValidationRuleFormatter": "Aminevsky\\ValidationRuleFormatter\\ValidationRuleFormatterFacade"
        }
      }
    }
}
